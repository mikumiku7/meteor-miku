{
  "schemaVersion": 1,
  "id": "meteor-miku",
  "version": "${version}",
  "name": "meteor-miku",
  "description": "Meteor Client addon miku.",
  "authors": [
    "mikumiku"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.github.mikumiku.addon.MikuMikuAddon"
    ]
  },
  "mixins": [
    "miku.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
